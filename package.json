{
    "name": "@riadh-adrani/recursive-web",
    "version": "0.8.0",
    "description": "Recursive Web is the web implementation of the Recursive Framework.",
    "license": "MIT",
    "main": "index.js",
    "type": "module",
    "types": "./index.d.ts",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/RiadhAdrani/recursive-web"
    },
    "scripts": {
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers 2",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
        "format": "npx prettier --write .",
        "check:format": "npx prettier --check .",
        "check:all": "npm run test && npm run check:format"
    },
    "keywords": [
        "npm",
        "package",
        "riadh",
        "adrani",
        "recursive",
        "web",
        "javascript",
        "front-end",
        "component-based",
        "framework"
    ],
    "dependencies": {
        "@riadh-adrani/recursive": "^0.8.0",
        "@riadh-adrani/utility-js": "^0.20.1",
        "@riadh-adrani/dom-control-js": "1.0.5"
    },
    "devDependencies": {
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "prettier": "^2.7.1",
        "cross-env": "^7.0.3"
    }
}
