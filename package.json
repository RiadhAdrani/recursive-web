{
    "name": "@riadh-adrani/recursive-web",
    "version": "0.7.61",
    "description": "Recursive Web is the web implementation of the Recursive Framework.",
    "license": "MIT",
    "main": "index.js",
    "types": "./index.d.ts",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/RiadhAdrani/recursive-web"
    },
    "scripts": {
        "start": "webpack-dev-server --config ./dev/webpack.dev.js --open",
        "test": "jest",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
        "generate": "node ./packages/scripts/generate.js && npm run format",
        "format": "npx prettier --write .",
        "check:format": "npx prettier --check .",
        "check:all": "npm run test && npm run check:format"
    },
    "keywords": [
        "npm",
        "package",
        "riadh",
        "adrani",
        "recursive",
        "web",
        "javascript",
        "front-end",
        "component-based",
        "framework"
    ],
    "dependencies": {
        "@riadh-adrani/recursive": "^0.7.58",
        "@riadh-adrani/utility-js": "^0.20.1",
        "@riadh-adrani/dom-control-js": "1.0.4"
    },
    "devDependencies": {
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "prettier": "^2.7.1"
    }
}
